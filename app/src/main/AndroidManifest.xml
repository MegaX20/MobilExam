<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobilexamain">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:noHistory="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ChoseActivity"/>
        <activity android:name=".MatchActivity" />
        <activity android:name=".ResetPasswordActivity" />
        <activity android:name=".RegistryActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".ReviewActivity" />
        <activity android:name=".QaActivity" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".RateActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>